<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="view_line_gray">
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:layout_marginBottom">10dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">#EEEEEE</item>

    </style>

    <style name="friend_tv_name_black">
        <item name="android:textSize">20dp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_marginLeft">15dp</item>
        <item name="android:textColor">#000</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="friend_tv_name_black_left" parent="friend_tv_name_black">
        <item name="android:layout_gravity">left</item>
    </style>

    <style name="friend_tv_name_gray_left" parent="friend_tv_name_black_left">
        <item name="android:textColor">#78281D1D</item>
        <item name="android:textSize">12dp</item>
    </style>



</resources>